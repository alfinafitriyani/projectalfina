<!DOCTYPE html>
<html>
<head>
  <title>Execution Context</title>
  <script>
    var x = 10;

    function outerFunction() {
      var y = 20;

      function innerFunction(z) {
        var result = x + y + z;
        console.log("Result:", result);
      }

      innerFunction(30);
    }

    function panggil() {
      var a = 5;
      var b = 15;

      function multiply() {
        var product = a * b;
        console.log("Product:", product);
      }

      multiply();
    }

    outerFunction();
    panggil();
  </script>
</head>
<body>
</body>
</html>
